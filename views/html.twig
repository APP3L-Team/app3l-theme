<!DOCTYPE html>
<html version="HTML+RDFa 1.1" lang="{{ language.language }}" dir="{{ language.dir }}" {{ rdf_namespaces }} >
	<head profile="{{ grddl_profile }}">
		{{ head }}
		<meta name="viewport" content="width=device-width, initial-scale=1">
		
		<title>{{ head_title }}</title>
		
		{{ styles }}

	</head>
	<body class="{{ classes }} container" {{ attributes }}>
		
		
		<!-- PAGE TOP -->
		
		<div id="page_top" >
			{{ page_top }}
		</div>
		
		
		<!-- PAGE -->
		
		<div id="page">
			{{ page }}
		</div>
		
		
		<!-- PAGE BOTTOM -->
		
		<div id="page_bottom">
			{{ page_bottom }}
		</div>
		
		{{ scripts }}
	</body>
</html>